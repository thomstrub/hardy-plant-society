"/Users/Thom/code/hardy-plant-society/src/components/ErrorMessage/ErrorMessage.jsx",[],"/Users/Thom/code/hardy-plant-society/src/utils/tokenService.js",["56"],"function setToken(token) {\n    if (token) {\n      localStorage.setItem('token', token);\n    } else {\n      localStorage.removeItem('token');\n    }\n  }\n  \n  function getToken() {\n    let token = localStorage.getItem('token');\n    if (token) {\n      // Check if expired, remove if it is\n      // atob is a function that decodes a base-64 string\n      const payload = JSON.parse(atob(token.split('.')[1]));\n      // JWT's exp is expressed in seconds, not milliseconds, so convert\n      if (payload.exp < Date.now() / 1000) {\n        localStorage.removeItem('token');\n        token = null;\n      }\n    }\n    return token;\n  }\n  \n  function getUserFromToken() {\n    const token = getToken();\n    return token ? JSON.parse(atob(token.split('.')[1])).user : null;\n  }\n  \n  function removeToken() {\n    localStorage.removeItem('token');\n  }\n  \n  export default {\n    setToken,\n    getToken,\n    removeToken,\n    getUserFromToken\n  };","/Users/Thom/code/hardy-plant-society/src/components/Forms/SignupForm/SignupForm.jsx",[],"/Users/Thom/code/hardy-plant-society/src/components/Forms/LoginForm/LoginForm.jsx",[],{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":12,"column":10,"nodeType":"63","messageId":"64","endLine":12,"endColumn":14},{"ruleId":"61","severity":1,"message":"65","line":19,"column":12,"nodeType":"63","messageId":"64","endLine":19,"endColumn":24},{"ruleId":"61","severity":1,"message":"66","line":7,"column":23,"nodeType":"63","messageId":"64","endLine":7,"endColumn":35},{"ruleId":"61","severity":1,"message":"66","line":8,"column":25,"nodeType":"63","messageId":"64","endLine":8,"endColumn":37},{"ruleId":"67","severity":1,"message":"68","line":44,"column":1,"nodeType":"69","endLine":49,"endColumn":3},{"ruleId":"67","seve